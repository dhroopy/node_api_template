{
  "env": {
    "node": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["eslint:recommended", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "prettier/prettier": "error",
    "no-process-exit": "off",
    "no-console": "warn"
  }
}
